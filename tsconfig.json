{
  "compilerOptions": {
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noImplicitOverride": true,
    "verbatimModuleSyntax": true,
    "noUncheckedIndexedAccess": true,
    "target": "esnext",
    "module": "preserve",
    "moduleDetection": "force",
    "lib": ["esnext"]
  }
}
