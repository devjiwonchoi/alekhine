{
  "name": "equine",
  "version": "0.2.1",
  "description": "Minimal Lichess API Client for Node.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "bunchee",
    "test": "NODE_ENV=test dotenv -e .env jest",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@swc/core": "^1.3.102",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "bunchee": "^5.0.1",
    "dotenv-cli": "^7.3.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vercel": "^33.5.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    },
    "testEnvironment": "node",
    "testTimeout": 10000,
    "verbose": true,
    "testPathIgnorePatterns": [
      "minors.test.ts",
      "bot.test.ts",
      "study.test.ts"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "author": "Jiwon Choi",
  "repository": "https://github.com/devjiwonchoi/equine.git",
  "bugs": "https://github.com/devjiwonchoi/equine/issues",
  "homepage": "https://equine.vercel.app",
  "files": [
    "dist"
  ],
  "keywords": [
    "chess",
    "lichess",
    "api",
    "client",
    "node"
  ],
  "engines": {
    "node": ">=18.17.0",
    "pnpm": "8.15.4"
  },
  "packageManager": "pnpm@8.15.4"
}
